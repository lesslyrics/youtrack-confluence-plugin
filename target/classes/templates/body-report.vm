<div class="yt yt-report">
<h3>$title</h3>
#set($headerEscapedHtml = $header)
#set($rowsEscapedHtml = $rows)
#set($paginationEscapedHtml = $pagination)
#if ($hasIssues)
<table>
    <tr>
      $headerEscapedHtml
    </tr>
    $rowsEscapedHtml
</table>
#else
<p>No more matching issues to display.</p>
#end
<p>
    $paginationEscapedHtml
</p>
</div>